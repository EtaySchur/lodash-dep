{
  "name": "lodash-example",
  "version": "1.0.0",
  "description": "Node.js app demonstrating Express and Lodash methods",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start:prod": "node dist/server.bundle.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prebuild": "npm run lint && npm run format",
    "prestart": "npm run build"
  },
  "dependencies": {
    "express": "4.17.1",
    "lodash": "3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@eslint/js": "^9.31.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "legacy",
    "express",
    "lodash",
    "nodejs"
  ],
  "author": "Legacy Developer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/legacy/lodash-example.git"
  },
  "bugs": {
    "url": "https://github.com/legacy/lodash-example/issues"
  },
  "homepage": "https://github.com/legacy/lodash-example#readme"
}
